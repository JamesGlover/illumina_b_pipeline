<% form_tag plate_search_path, :method => 'get' do %>
  <p>
    <%= label_tag :user_barcode, "User Barcode" %>
    <%= text_field_tag :user_barcode, params[:user_barcode] %>
  </p>
  <p>
    <%= label_tag :plate_barcode, "Plate Barcode" %>
    <%= text_field_tag :plate_barcode, params[:plate_barcode] %>
  </p>
    <%= submit_tag "Search", :name => nil, :class => "ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only" %>
<% end %>
